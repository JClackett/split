{
  "name": "@split/web",
  "version": "1.0.0",
  "main": "src/index.ts",
  "license": "MIT",
  "scripts": {
    "start": "parcel src/index.html --port 6969",
    "build": "rm -rf dist && NODE_ENV=production parcel build src/index.html",
    "test": "jest --passWithNoTests",
    "typecheck": "tsc --noEmit",
    "lint": "eslint --cache --ext .jsx,.js,.ts,.tsx src",
    "prettify": "prettier --write './src/**/*.{ts,tsx}'",
    "pre-commit": "lint-staged"
  },
  "lint-staged": {
    "*.{js,jsx,json,ts,tsx,html}": [
      "prettier --write",
      "git add"
    ],
    "*.{js,jsx,ts,tsx}": [
      "eslint --fix --cache --ext .jsx,.js,.ts,.tsx src",
      "git add"
    ]
  },
  "dependencies": {
    "@reach/router": "^1.2.1",
    "@split/connector": "1.0.0",
    "apollo-boost": "^0.3.1",
    "axios": "^0.18.0",
    "dayjs": "^1.8.11",
    "graphql": "^14.1.1",
    "lodash.throttle": "^4.1.1",
    "logrocket": "^0.6.19",
    "logrocket-react": "^3.0.0",
    "polished": "^3.0.4",
    "query-string": "^6.4.0",
    "react": "16.8.6",
    "react-apollo-hooks": "0.4.5",
    "react-dom": "16.8.6",
    "react-dropzone": "^10.0.5",
    "react-error-boundary": "^1.2.3",
    "react-media": "^1.9.2",
    "styled-components": "^4.1.3",
    "styled-media-query": "^2.1.1",
    "unfetch": "^4.1.0",
    "use-media": "^1.3.0"
  },
  "devDependencies": {
    "@types/graphql": "14.2.0",
    "@types/jest": "24.0.13",
    "@types/lodash.throttle": "4.1.6",
    "@types/node": "11.13.11",
    "@types/reach__router": "1.2.4",
    "@types/react": "16.8.18",
    "@types/react-dom": "16.8.4",
    "@types/react-dropzone": "4.2.2",
    "@types/styled-components": "4.1.15",
    "@typescript-eslint/eslint-plugin": "1.9.0",
    "@typescript-eslint/parser": "1.9.0",
    "babel-eslint": "10.0.1",
    "eslint": "5.16.0",
    "eslint-config-prettier": "4.3.0",
    "eslint-config-react-app": "4.0.1",
    "eslint-plugin-flowtype": "3.9.1",
    "eslint-plugin-import": "2.17.3",
    "eslint-plugin-jsx-a11y": "6.2.1",
    "eslint-plugin-react": "7.13.0",
    "eslint-plugin-react-hooks": "1.6.0",
    "jest": "24.8.0",
    "jest-dom": "3.4.0",
    "parcel-bundler": "1.12.3",
    "prettier": "1.17.1",
    "react-testing-library": "7.0.1",
    "ts-jest": "24.0.2",
    "typescript": "3.4.5"
  }
}
